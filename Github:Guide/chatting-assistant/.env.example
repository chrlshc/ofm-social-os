# OnlyFans Chatting Assistant Configuration

# Account Settings
CHATTING_ACCOUNT_SIZE=small
CHATTING_LANGUAGE=en
CHATTING_MANUAL_SEND=true

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/ofm_db

# Cache Configuration (optional)
REDIS_URL=redis://localhost:6379/0

# NLP Configuration
SPACY_MODEL=en_core_web_sm

# API Configuration (for server mode)
CHATTING_API_HOST=0.0.0.0
CHATTING_API_PORT=8001

# Logging
LOG_LEVEL=INFO
LOG_FILE=/var/log/ofm/chatting.log

# Compliance Settings
CHATTING_COMPLIANCE_MODE=true
CHATTING_AI_DISCLOSURE=false

# Performance Settings
CHATTING_CACHE_TTL=3600
CHATTING_MAX_MESSAGE_LENGTH=1000
CHATTING_RATE_LIMIT_PER_MINUTE=60

# Advanced Personalization Settings
CHATTING_REAL_TIME_PERSONALIZATION=true
CHATTING_ACTIVITY_TRACKING=true
CHATTING_AFFINITY_LEARNING=true
CHATTING_ACTIVITY_WEIGHT=0.8
CHATTING_EMOTION_WEIGHT=0.6
CHATTING_TIMING_WEIGHT=0.4

# Emotion Analysis Configuration
CHATTING_EMOTION_ANALYSIS=true
CHATTING_EMOTION_MODEL=bhadresh-savani/distilbert-base-uncased-emotion
CHATTING_EMOTION_CONFIDENCE_THRESHOLD=0.7
CHATTING_TONE_ADAPTATION=true
CHATTING_EMOTIONAL_MEMORY_DAYS=30

# A/B Testing Configuration
CHATTING_AB_TESTING=true
CHATTING_AB_STRATEGY=thompson_sampling
CHATTING_AB_EXPLORATION_RATE=0.15
CHATTING_AB_MIN_SAMPLE_SIZE=10
CHATTING_AB_CONFIDENCE_THRESHOLD=0.95
CHATTING_AB_CACHE_TTL_MINUTES=30

# Multi-Agent Learning
CHATTING_MULTI_AGENT_API=true
CHATTING_AGENT_KEY=CHANGE_THIS_TO_SECURE_KEY_IN_PRODUCTION
CHATTING_AGENT_SESSION_TTL_HOURS=24
CHATTING_KNOWLEDGE_SHARING=true
CHATTING_PROFILE_SYNCHRONIZATION=true
CHATTING_MAX_CONCURRENT_AGENTS=50

# ML Training Pipeline
CHATTING_ML_TRAINING=true
CHATTING_ML_AUTO_RETRAIN=true
CHATTING_ML_MIN_TRAINING_SAMPLES=50
CHATTING_ML_RETRAIN_THRESHOLD=50
CHATTING_ML_MODEL_MAX_AGE_DAYS=7
CHATTING_USE_TRANSFORMERS=true
CHATTING_USE_CUSTOM_MODELS=false

# Dynamic Templates
CHATTING_USE_DYNAMIC_TEMPLATES=true
CHATTING_TEMPLATE_EFFECTIVENESS_THRESHOLD=0.5
CHATTING_TEMPLATE_CACHE_TTL_MINUTES=15
CHATTING_TEMPLATE_AB_TESTING=true

# One-Click Sending
CHATTING_ONE_CLICK_SENDING=true
CHATTING_CLIPBOARD_SUPPORT=true
CHATTING_AUTO_OPEN_BROWSER=true
CHATTING_AUDIT_RETENTION_DAYS=30

# Integration Settings (for OFM Social OS)
OFM_API_URL=http://localhost:3000/api
OFM_API_KEY=your_api_key_here

# Monitoring (optional)
PROMETHEUS_METRICS=false
OPENTELEMETRY_ENDPOINT=http://localhost:4317