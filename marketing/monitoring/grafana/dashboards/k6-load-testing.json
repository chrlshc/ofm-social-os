{
  "dashboard": {
    "id": null,
    "title": "K6 Load Testing - OFM Social OS",
    "tags": ["k6", "load-testing", "performance", "ofm"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "schemaVersion": 30,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(up{job=\"k6\"}[1m]) > 0",
          "iconColor": "green",
          "name": "Test Start/Stop",
          "titleFormat": "K6 Test Event"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "ðŸŽ¯ Test Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "k6_vus",
            "legendFormat": "Active VUs",
            "refId": "A"
          },
          {
            "expr": "rate(k6_http_reqs_total[1m]) * 60",
            "legendFormat": "Requests/min",
            "refId": "B"
          },
          {
            "expr": "k6_http_req_failed / k6_http_reqs_total * 100",
            "legendFormat": "Error Rate %",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 90}
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "auto",
          "colorMode": "value"
        }
      },
      {
        "id": 2,
        "title": "ðŸ“ˆ HTTP Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(k6_http_reqs_total[1m]) * 60",
            "legendFormat": "Total RPS",
            "refId": "A"
          },
          {
            "expr": "rate(k6_http_reqs_total{endpoint=\"publish\"}[1m]) * 60",
            "legendFormat": "Publish RPS",
            "refId": "B"
          },
          {
            "expr": "rate(k6_http_reqs_total{endpoint=\"upload\"}[1m]) * 60",
            "legendFormat": "Upload RPS",
            "refId": "C"
          },
          {
            "expr": "rate(k6_http_reqs_total{endpoint=\"webhook\"}[1m]) * 60",
            "legendFormat": "Webhook RPS",
            "refId": "D"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 6
        },
        "yAxes": [
          {
            "label": "Requests per minute",
            "show": true
          }
        ],
        "xAxes": [
          {
            "show": true,
            "type": "time"
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true
        }
      },
      {
        "id": 3,
        "title": "âš¡ Response Times (Percentiles)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(k6_http_req_duration_bucket[5m])) * 1000",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(k6_http_req_duration_bucket[5m])) * 1000",
            "legendFormat": "P95 (SLO: <10s)",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(k6_http_req_duration_bucket[5m])) * 1000",
            "legendFormat": "P99 (SLO: <15s)",
            "refId": "C"
          },
          {
            "expr": "avg(rate(k6_http_req_duration_sum[5m]) / rate(k6_http_req_duration_count[5m])) * 1000",
            "legendFormat": "Average",
            "refId": "D"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 6
        },
        "yAxes": [
          {
            "label": "Milliseconds",
            "show": true,
            "logBase": 1
          }
        ],
        "xAxes": [
          {
            "show": true,
            "type": "time"
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true
        },
        "thresholds": [
          {
            "value": 10000,
            "colorMode": "critical",
            "op": "gt",
            "fill": false,
            "line": true,
            "yaxis": "left"
          },
          {
            "value": 15000,
            "colorMode": "critical",
            "op": "gt",
            "fill": true,
            "line": true,
            "yaxis": "left"
          }
        ]
      },
      {
        "id": 4,
        "title": "âŒ Error Rates by Endpoint",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(k6_http_req_failed{endpoint=\"publish\"}[1m]) / rate(k6_http_reqs_total{endpoint=\"publish\"}[1m]) * 100",
            "legendFormat": "Publish Error Rate",
            "refId": "A"
          },
          {
            "expr": "rate(k6_http_req_failed{endpoint=\"upload\"}[1m]) / rate(k6_http_reqs_total{endpoint=\"upload\"}[1m]) * 100",
            "legendFormat": "Upload Error Rate",
            "refId": "B"
          },
          {
            "expr": "rate(k6_http_req_failed{endpoint=\"webhook\"}[1m]) / rate(k6_http_reqs_total{endpoint=\"webhook\"}[1m]) * 100",
            "legendFormat": "Webhook Error Rate",
            "refId": "C"
          },
          {
            "expr": "rate(k6_http_req_failed[1m]) / rate(k6_http_reqs_total[1m]) * 100",
            "legendFormat": "Overall Error Rate",
            "refId": "D"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 14
        },
        "yAxes": [
          {
            "label": "Error Rate (%)",
            "show": true,
            "max": 5,
            "min": 0
          }
        ],
        "xAxes": [
          {
            "show": true,
            "type": "time"
          }
        ],
        "thresholds": [
          {
            "value": 1,
            "colorMode": "critical",
            "op": "gt",
            "fill": false,
            "line": true,
            "yaxis": "left"
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "max": true
        }
      },
      {
        "id": 5,
        "title": "ðŸ‘¥ Virtual Users & Data Transfer",
        "type": "graph",
        "targets": [
          {
            "expr": "k6_vus",
            "legendFormat": "Active VUs",
            "refId": "A",
            "yAxis": 1
          },
          {
            "expr": "rate(k6_data_sent_bytes[1m])",
            "legendFormat": "Data Sent (bytes/s)",
            "refId": "B",
            "yAxis": 2
          },
          {
            "expr": "rate(k6_data_received_bytes[1m])",
            "legendFormat": "Data Received (bytes/s)",
            "refId": "C",
            "yAxis": 2
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 14
        },
        "yAxes": [
          {
            "label": "Virtual Users",
            "show": true
          },
          {
            "label": "Bytes per second",
            "show": true,
            "opposite": true
          }
        ],
        "xAxes": [
          {
            "show": true,
            "type": "time"
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true
        }
      },
      {
        "id": 6,
        "title": "ðŸš€ Platform-Specific Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(k6_http_req_duration_bucket{platform=\"instagram\"}[5m])) * 1000",
            "legendFormat": "Instagram P95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(k6_http_req_duration_bucket{platform=\"tiktok\"}[5m])) * 1000",
            "legendFormat": "TikTok P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(k6_http_req_duration_bucket{platform=\"reddit\"}[5m])) * 1000",
            "legendFormat": "Reddit P95",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.95, rate(k6_http_req_duration_bucket{platform=\"meta\"}[5m])) * 1000",
            "legendFormat": "Meta P95",
            "refId": "D"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 22
        },
        "yAxes": [
          {
            "label": "Response Time (ms)",
            "show": true
          }
        ],
        "xAxes": [
          {
            "show": true,
            "type": "time"
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true
        }
      },
      {
        "id": 7,
        "title": "ðŸ’¾ Custom Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(k6_publish_latency_bucket[5m]))",
            "legendFormat": "Publish Latency P95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(k6_upload_latency_bucket[5m]))",
            "legendFormat": "Upload Latency P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(k6_webhook_latency_bucket[5m]))",
            "legendFormat": "Webhook Latency P95",
            "refId": "C"
          },
          {
            "expr": "rate(k6_api_calls_total[1m])",
            "legendFormat": "API Calls/sec",
            "refId": "D"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 22
        },
        "yAxes": [
          {
            "label": "Milliseconds / Calls per second",
            "show": true
          }
        ],
        "xAxes": [
          {
            "show": true,
            "type": "time"
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true
        }
      },
      {
        "id": 8,
        "title": "ðŸ§ª Test Scenario Breakdown",
        "type": "table",
        "targets": [
          {
            "expr": "sum by (test_type) (k6_vus)",
            "legendFormat": "{{test_type}}",
            "refId": "A",
            "instant": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 30
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "test_type": "Test Type",
                "Value": "Active VUs"
              }
            }
          }
        ]
      },
      {
        "id": 9,
        "title": "â±ï¸ Soak Test Progress",
        "type": "stat",
        "targets": [
          {
            "expr": "k6_vus{test_type=\"soak\"}",
            "legendFormat": "Soak Test VUs",
            "refId": "A"
          },
          {
            "expr": "avg_over_time(k6_http_req_duration_avg{test_type=\"soak\"}[10m]) * 1000",
            "legendFormat": "Avg Response Time (10m)",
            "refId": "B"
          },
          {
            "expr": "rate(k6_http_req_failed{test_type=\"soak\"}[5m]) / rate(k6_http_reqs_total{test_type=\"soak\"}[5m]) * 100",
            "legendFormat": "Error Rate %",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 30
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5000},
                {"color": "red", "value": 10000}
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "vertical",
          "textMode": "auto",
          "colorMode": "value"
        }
      },
      {
        "id": 10,
        "title": "ðŸŽ¯ SLO Compliance",
        "type": "singlestat",
        "targets": [
          {
            "expr": "(histogram_quantile(0.95, rate(k6_http_req_duration_bucket[5m])) * 1000) < 10000",
            "legendFormat": "P95 < 10s",
            "refId": "A"
          },
          {
            "expr": "(histogram_quantile(0.99, rate(k6_http_req_duration_bucket[5m])) * 1000) < 15000",
            "legendFormat": "P99 < 15s",
            "refId": "B"
          },
          {
            "expr": "(rate(k6_http_req_failed[1m]) / rate(k6_http_reqs_total[1m]) * 100) < 1",
            "legendFormat": "Error Rate < 1%",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 30
        },
        "valueName": "current",
        "valueMaps": [
          {"value": "1", "text": "âœ… PASS"},
          {"value": "0", "text": "âŒ FAIL"}
        ],
        "colorBackground": true,
        "colorValue": false,
        "colors": ["#d44a3a", "#299c46", "#d44a3a"],
        "thresholds": "0.5,1"
      },
      {
        "id": 11,
        "title": "ðŸ“Š Test Execution Log",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"k6\"}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 38
        },
        "options": {
          "showTime": true,
          "showLabels": false,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "sortOrder": "Descending"
        }
      }
    ],
    "variables": [
      {
        "name": "test_type",
        "type": "query",
        "label": "Test Type",
        "query": "label_values(k6_http_reqs_total, test_type)",
        "datasource": "prometheus",
        "multi": false,
        "includeAll": true,
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "options": [],
        "refresh": "on_time_range_change"
      },
      {
        "name": "endpoint",
        "type": "query",
        "label": "Endpoint",
        "query": "label_values(k6_http_reqs_total, endpoint)",
        "datasource": "prometheus",
        "multi": false,
        "includeAll": true,
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "options": [],
        "refresh": "on_time_range_change"
      },
      {
        "name": "platform",
        "type": "query",
        "label": "Platform",
        "query": "label_values(k6_http_reqs_total, platform)",
        "datasource": "prometheus",
        "multi": false,
        "includeAll": true,
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "options": [],
        "refresh": "on_time_range_change"
      }
    ]
  }
}