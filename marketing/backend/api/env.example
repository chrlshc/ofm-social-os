# Database
DATABASE_URL=postgresql://user:password@localhost:5432/ofm_marketing

# Temporal
TEMPORAL_ADDRESS=localhost:7233
TEMPORAL_NAMESPACE=default
TEMPORAL_TLS=false

# Authentication
JWT_SECRET=your-secret-key

# Scraper Configuration
ENABLE_AUTOMATION=true
SCRAPER_CRON=0 3 * * *
# Limit profiles per platform (optional)
SCRAPER_PROFILE_LIMITS={"instagram":10,"tiktok":10,"reddit":5}

# Twitter API (optional)
TWITTER_BEARER_TOKEN=

# Service Configuration
PORT=3000
NODE_ENV=development
SERVICE_VERSION=1.0.0

# Logging
LOG_LEVEL=info

# Priority Creators (comma-separated user IDs)
PRIORITY_CREATORS=

# Platform-specific queues
USE_PLATFORM_QUEUES=false