{
  "name": "ofm-social-api",
  "version": "1.0.0",
  "description": "OFM Social OS - AI Agent-Based Social Media Publisher API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "migrate": "node -r tsx/cjs src/scripts/migrate.ts",
    "doctor": "tsx src/scripts/doctor.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "ioredis": "^5.3.2",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "zod": "^3.22.4",
    "@opentelemetry/sdk-node": "^0.45.1",
    "@opentelemetry/resources": "^1.17.0",
    "@opentelemetry/semantic-conventions": "^1.17.0",
    "@opentelemetry/exporter-otlp-http": "^0.45.1",
    "@opentelemetry/sdk-metrics": "^1.17.0",
    "@opentelemetry/auto-instrumentations-node": "^0.39.4",
    "@opentelemetry/api": "^1.6.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.7",
    "@types/pg": "^8.10.7",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.6",
    "ts-jest": "^29.1.1",
    "tsx": "^4.1.4",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "social-media",
    "ai-agents",
    "temporal",
    "multi-platform",
    "publishing"
  ],
  "license": "MIT"
}