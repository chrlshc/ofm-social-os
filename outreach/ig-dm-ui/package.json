{
  "name": "ig-dm-ui",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "echo 'TypeScript compilation'",
    "dm": "node src/cli-dm.js",
    "dm-with-likes": "PRE_ENGAGE_LIKES=2 node src/cli-dm.js",
    "dm-random": "node src/cli-dm.js --random-message --preengage likes=2",
    "dm-templates": "node src/cli-dm.js --list-messages",
    "gen-dm-pack": "node scripts/one_run_dm_pack.mjs",
    "test-apify": "node src/test-apify.js",
    "test-headless": "node src/headless-switch.js --test",
    "report": "node src/batch-reporter.js --generate",
    "status": "node src/cli-dm.js --status",
    "install-prod": "npm ci --omit=dev && npm audit --production",
    "audit-deps": "npm audit --audit-level=moderate",
    "audit-prod": "npm audit --production --audit-level=high",
    "check-updates": "npm outdated",
    "security-check": "npm ci --omit=dev && npm audit --production --audit-level=high",
    "test": "echo 'Installation successful!'",
    "apify:hashtag": "node src/apify-discover-export.mjs hashtag --q onlyfanscreator --limit 200 --out ../../out/dm_todo.csv",
    "apify:export": "node src/apify-discover-export.mjs export --in ../../out/apify_dataset.json --out ../../out/dm_todo.csv",
    "apify:us": "node src/apify-discover-export.mjs hashtag --hashtags onlyfansusa,ugcmodel,miamimodel,nycmodel,lasvegasmodel,atlantamodel,californiacreator,texascreator --limit 150 --minFollowers 4000 --minScore 2 --top 300 --usOnly true --tzDefault ET --out ../../out/dm_todo_us.csv",
    "apify:us-intent": "node src/apify-discover-export.mjs hashtag --hashtags collab,branddeals,creatorcommunity,ugcmodel,creatorlife --limit 150 --minFollowers 4000 --minScore 2 --top 300 --usOnly true --tzDefault ET --out ../../out/dm_todo_us_intent.csv",
    "beta:setup": "node src/cli-dm-beta.mjs setup",
    "beta:campaign": "node src/cli-dm-beta.mjs campaign",
    "beta:status": "node src/cli-dm-beta.mjs status",
    "beta:test": "node src/cli-dm-beta.mjs test",
    "natural:setup": "node src/cli-dm-natural.mjs setup",
    "natural:add-warm": "node src/cli-dm-natural.mjs add-warm",
    "natural:campaign": "node src/cli-dm-natural.mjs campaign",
    "natural:status": "node src/cli-dm-natural.mjs status",
    "natural:test": "node src/cli-dm-natural.mjs test",
    "fast:blast": "node src/cli-fast-tempo.mjs blast",
    "fast:handoff": "node src/cli-fast-tempo.mjs handoff",
    "fast:stats": "node src/cli-fast-tempo.mjs stats",
    "enhanced:campaign": "node src/cli-enhanced.mjs campaign",
    "enhanced:check-replies": "node src/cli-enhanced.mjs check-replies",
    "enhanced:stats": "node src/cli-enhanced.mjs stats",
    "enhanced:accounts": "node src/cli-enhanced.mjs accounts",
    "enhanced:test": "node src/cli-enhanced.mjs test-message",
    "enhanced:db-init": "node src/cli-enhanced.mjs db-init",
    "enhanced:db-stats": "node src/cli-enhanced.mjs db-stats"
  },
  "dependencies": {
    "apify": "^3.4.4",
    "apify-client": "^2.13.0",
    "crawlee": "^3.14.1",
    "csv-writer": "^1.6.0",
    "dotenv": "16.4.5",
    "fingerprint-injector": "2.1.69",
    "ghost-cursor": "1.4.1",
    "mustache": "^4.2.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3",
    "pino": "9.3.2",
    "pino-pretty": "11.2.2",
    "puppeteer": "24.9.0",
    "puppeteer-extra": "3.3.6",
    "puppeteer-extra-plugin-stealth": "2.11.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/node": "22.8.0",
    "ts-node": "10.9.2",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "private"
  },
  "author": "Claude AI Assistant",
  "license": "UNLICENSED"
}
