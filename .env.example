# Database
DATABASE_URL_OFM_PRODUCTION=postgresql://user:pass@host:5432/db?sslmode=require
CRYPTO_MASTER_KEY=generate_with_openssl_rand_base64_32

# Authentication
NEXTAUTH_SECRET=generate_with_openssl_rand_base64_32
NEXTAUTH_URL=http://localhost:3000

# Reddit OAuth
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret
REDDIT_USER_AGENT=ofm-social-os:v1.0.0 (by /u/your_username)
REDDIT_REDIRECT_URI=http://localhost:3000/api/social/auth/reddit/callback

# Instagram OAuth
INSTAGRAM_CLIENT_ID=your_instagram_client_id
INSTAGRAM_CLIENT_SECRET=your_instagram_client_secret

# TikTok OAuth
TIKTOK_CLIENT_KEY=your_tiktok_client_key
TIKTOK_CLIENT_SECRET=your_tiktok_client_secret

# Stripe
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# AWS KMS (optional - leave empty to use local encryption)
AWS_REGION=
AWS_KMS_KEY_ID=

# App
NEXT_PUBLIC_URL=http://localhost:3000
NODE_ENV=development
EOF < /dev/null